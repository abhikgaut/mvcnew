@model IEnumerable<mvcnew.Models.EMPDATA>
@{
    ViewBag.Title = "getDeptData";
}

<h2>getDeptData</h2>
@using (Html.BeginForm("getDept","databse"))
{ 
    <h3>
        enter the dept no:@Html.TextBox("txtdno",Request["txtdno"])
    </h3>
    <input type="submit" value="getdata"/>
    if (Model != null)
    { 
<table>
    <tr>
        <td>Emp no</td>
        <td>Emp name</td>
        <td>job</td>
        <td>manager</td>
        <td>hiredate</td>
        <td>salary</td>
        <td>commission</td>
        <td>dept no</td>
    </tr>
    @foreach(var E in Model)
    {
    <tr>
        <td>@E.EMPNO</td>
        <td>@E.ENAME</td>
        <td>@E.JOB</td>
        <td>@E.MGR</td>
        <td>@E.HIREDATE</td>
        <td>@E.SAL</td>
        <td>@E.COMM</td>
        <td>@E.DEPTNO</td>
    </tr>
    }
</table>
    }
}

